            <string>/// Get highscore
highscore = 0;
current_score = 0;

ini_open("score.ini");
highscore = ini_read_real("scoring", "highscore",0);
current_score = ini_read_real("scoring", "current_score",0);

if (current_score &gt; highscore) {
